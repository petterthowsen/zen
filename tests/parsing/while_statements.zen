// Basic while loop
while x < 10 {
    print(x)
    x = x + 1
}

// While loop with break
while true {
    if x > 100 {
        break
    }
    print(x)
    x = x + 1
}

// While loop with continue
while y < 20 {
    y = y + 1
    if y < 10 {
        continue
    }
    print(y)
}

// While loop with complex condition
while x > 0 and y < 100 {
    print(x)
    print(y)
    x = x - 1
    y = y + 1
}

// Nested while loops
while i < 5 {
    j = 0
    while j < 3 {
        if i == 2 {
            break
        }
        print(i)
        print(j)
        j = j + 1
    }
    i = i + 1
}
